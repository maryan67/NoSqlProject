<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.gui.RegisterController">
   <children>
      <Label layoutX="76.0" layoutY="46.0" text="Register">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="76.0" layoutY="121.0" promptText="Username" />
      <PasswordField fx:id="txtPass1" layoutX="76.0" layoutY="179.0" promptText="Password" />
      <PasswordField fx:id="txtPass2" layoutX="76.0" layoutY="238.0" promptText="Repeat Password" />
      <Button fx:id="btnRegister" layoutX="76.0" layoutY="280.0" mnemonicParsing="false" onAction="#getUserData" prefWidth="75.0" text="Register" />
      <Button fx:id="btnBack" layoutX="151.0" layoutY="280.0" mnemonicParsing="false" onAction="#goBackToLogin" prefWidth="75.0" text="Back" />
      <Label fx:id="lblUserWarning" layoutX="76.0" layoutY="146.0" textFill="RED" visible="false" />
      <Label fx:id="lblPass1Warning" layoutX="76.0" layoutY="204.0" textFill="RED" visible="false" />
      <Label fx:id="lblPass2Warning" layoutX="76.0" layoutY="263.0" textFill="RED" visible="false" />
   </children>
</AnchorPane>
