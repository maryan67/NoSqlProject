<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.gui.MainControler">
   <children>
      <Button fx:id="btnLogin" layoutX="76.0" layoutY="199.0" mnemonicParsing="false" prefWidth="75.0" text="Login" />
      <Button fx:id="btnRegister" layoutX="152.0" layoutY="199.0" mnemonicParsing="false" onAction="#goToRegister" prefWidth="75.0" text="Register" />
      <TextField fx:id="txtUsername" layoutX="77.0" layoutY="99.0" promptText="Username" />
      <PasswordField fx:id="txtPassword" layoutX="76.0" layoutY="150.0" promptText="Password" />
      <Label fx:id="lblLogin" layoutX="76.0" layoutY="42.0" text="Login">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
