<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.gui.EditProfileController">
   <children>
      <Label fx:id="lblWelcome" layoutX="65.0" layoutY="51.0" text="Edit Information">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtEmail" layoutX="76.0" layoutY="116.0" promptText="Email" />
      <TextField fx:id="txtStreet" layoutX="76.0" layoutY="177.0" promptText="Street" />
      <TextField fx:id="txtStrNr" layoutX="76.0" layoutY="236.0" prefHeight="25.0" prefWidth="60.0" promptText="Str Nr" />
      <TextField fx:id="txtBlckNr" layoutX="165.0" layoutY="236.0" prefWidth="60.0" promptText="Block Nr" />
      <TextField fx:id="txtZip" layoutX="76.0" layoutY="294.0" promptText="ZIP Code" />
      <TextArea fx:id="txtBio" layoutX="50.0" layoutY="354.0" prefHeight="80.0" prefWidth="200.0" promptText="Bio" />
      <Button fx:id="btnConfirm" layoutX="50.0" layoutY="482.0" mnemonicParsing="false" onAction="#onConfirmClicked" prefWidth="75.0" text="Confirm" />
      <Button fx:id="BtnCancel" layoutX="175.0" layoutY="482.0" mnemonicParsing="false" onAction="#onCancelClick" prefWidth="75.0" text="Cancel" />
   </children>
</AnchorPane>
